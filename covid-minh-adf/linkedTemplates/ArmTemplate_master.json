{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "covid-minh-adf"
		},
		"ls_ablob_covidminhsa_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_ablob_covidminhsa'"
		},
		"ls_adls_covidminhdl_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_adls_covidminhdl'"
		},
		"ls_covid_databricks_accessToken": {
			"type": "secureString",
			"metadata": "Secure string for 'accessToken' of 'ls_covid_databricks'"
		},
		"ls_covid_sqldb_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_covid_sqldb'"
		},
		"ls_adls_covidminhdl_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://covidminhdl.dfs.core.windows.net/"
		},
		"ls_covid_databricks_properties_typeProperties_existingClusterId": {
			"type": "string",
			"defaultValue": "0806-105053-oqv5mccb"
		},
		"ls_http_ecdc_data_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "@{linkedService().sourceBaseURL}"
		},
		"tr_population_data_arrived_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/6a363c1f-dc6a-4692-9e5c-658d2a49b737/resourceGroups/covid-reporting-rg/providers/Microsoft.Storage/storageAccounts/covidminhsa"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "covid-minh-adf_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_ablob_covidminhsa_connectionString": {
						"value": "[parameters('ls_ablob_covidminhsa_connectionString')]"
					},
					"ls_adls_covidminhdl_accountKey": {
						"value": "[parameters('ls_adls_covidminhdl_accountKey')]"
					},
					"ls_covid_databricks_accessToken": {
						"value": "[parameters('ls_covid_databricks_accessToken')]"
					},
					"ls_covid_sqldb_connectionString": {
						"value": "[parameters('ls_covid_sqldb_connectionString')]"
					},
					"ls_adls_covidminhdl_properties_typeProperties_url": {
						"value": "[parameters('ls_adls_covidminhdl_properties_typeProperties_url')]"
					},
					"ls_covid_databricks_properties_typeProperties_existingClusterId": {
						"value": "[parameters('ls_covid_databricks_properties_typeProperties_existingClusterId')]"
					},
					"ls_http_ecdc_data_properties_typeProperties_url": {
						"value": "[parameters('ls_http_ecdc_data_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "covid-minh-adf_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"tr_population_data_arrived_properties_typeProperties_scope": {
						"value": "[parameters('tr_population_data_arrived_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'covid-minh-adf_ArmTemplate_0')]"
			]
		},
		{
			"name": "covid-minh-adf_ArmTemplate_2",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_2.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'covid-minh-adf_ArmTemplate_1')]"
			]
		}
	]
}